    "run-series": "^1.1.9",
    "run-waterfall": "^1.1.7",
    "standard": "^16.0.4",
    "tape": "^4.7.1",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "lint": "eslint --ext .ts,.js src bin test",
    "pretest": "rimraf test/work",
